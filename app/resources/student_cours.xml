<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.StudentCourse" table="STUDENT_COURSE">
        <id name="id" type="int">
            <column name="ID" precision="5" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="courseName" type="string">
            <column name="COURSE_NAME" length="100" not-null="true" />
        </property>
        <property name="recordBookNumber" type="string">
            <column name="RECORD_BOOK_NUMBER" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>